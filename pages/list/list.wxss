.faq-wrap {
    text-align: center;
    padding: 10px 10px 50px 10px;
}
.bgcolor{
  background:#f1f1f1;
}
.nodata{
  text-align:center;padding:30px 10px;color:#ccc;
}
.list {
    padding: 10px 0;
    border-radius: 4px;
    box-shadow:0 0 8px #ccc;
    background:#fff;
    text-align: left;
    margin-bottom: 20px;
}

.status {
    font-size: 14px;
    font-weight: normal;
    border:1px solid #ccc;
    background:#eee;
    color: #ccc;
    line-height:20px;
    padding: 0 5px;
    border-radius: 5px;
}
.avatar {
    float:left;
    border-radius: 3px;
    margin: 2px 3px 0 0;
    width: 16px;
    height: 16px;
}

.status-reply {
    border:1px solid #179b16;
    background:#fff;
    color: #179b16;
}

.status-wran {
    border:1px solid #eaa015;
    background:#fff;
    color: #eaa015;
}
.org,.project{
    font-size:14px;
    font-weight:normal;
    margin-left:10px;
    padding: 0 5px;
    line-height:20px;
    display:inline-block;
    border-radius: 5px;
}
.org{
    border:1px solid #78c2f4;
    background:#fff;
    color: #78c2f4;
}
.project{
    border:1px solid #2bb20b;
    background:#fff;
    color: #2bb20b; 
}
.title {
    padding: 0 10px 10px 10px;
    font-size: 16px;
    font-weight: bold;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cont {
    font-size: 14px;
    padding: 0 10px;
    color: #666;
    overflow: hidden;
    max-height: 60px;
}

.info {
    margin-top: 5px;
    padding: 10px 0 0 10px;
    font-size: 14px;
    height: 20px;
    color: #1aad16;
    border-top: 1px solid #ddd;
}

.time {
    float: right;
    text-align: center;
    /* border-left: 1px solid #ddd; */
    width: 55%;
}

.tools {
    width: 100%;
    position: fixed;
    bottom: 10px;
    line-height: 30px;
}

.button {
    margin: 10px 10px 0 10px;
    text-align: center;
}

.blank {
    width: 100%;
    height: 10px;
    background: #fff;
    display: block;
    position: fixed;
    bottom: 0;
}
